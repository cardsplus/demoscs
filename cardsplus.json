{
    "wiki" : {
        "serverURI": "https://cardsplus.atlassian.net/wiki/rest/api",
        "domain": "demoscs",
        "space": "DEMOSCS"
    },
    "allLink": {
        ".*": "HEAD"
    },
    "allAdocElement": [
        {
            "filename": "doc/EsySpec.adoc",
            "style": "spec"
        },
        {
            "filename": "glob:doc/Esy*Api.adoc",
            "style": "service",
            "leveloffset": 1
        }
    ],
    "allJavaElement": [
        {
            "module": "app/backend-api",
            "directory": "src/main/java",
            "allFilename" : [{
                "style": "entity"
            }]
        },
        {
            "module": "app/backend",
            "directory": "src/main/java",
            "allFilename" : [{
                "style": "tagged"
            }]
        }
    ],
    "allPageStyler": [
        {
            "name": "spec",
            "titleStrategy": "TITLE"
        },
        {
            "name": "service",
            "titleStrategy": "SCOPE",
            "script": "@cards/service.ctml"
        },   
        {
            "name": "entity",
            "titleStrategy": "PATH",
            "script": "@cards/entity.ctml"
        },
        {
            "name": "tagged",
            "titleStrategy": "PATH",
            "script": "@cards/tagged.ctml"
        } 
    ]
}
